        <script>
            // Add event listeners for view buttons
            document.querySelectorAll('[data-bs-target="#viewUserModal"]').forEach(button => {
                button.addEventListener('click', function() {
                    document.getElementById('viewUserId').textContent = this.getAttribute('data-user-id');
                    document.getElementById('viewUserName').textContent = this.getAttribute('data-user-name');
                    document.getElementById('viewUserTier').textContent = this.getAttribute('data-user-tier');
                    document.getElementById('viewStartWork').textContent = this.getAttribute('data-start-work');
                    document.getElementById('viewUserRole').textContent = this.getAttribute('data-user-role');
                    document.getElementById('viewUserEmail').textContent = this.getAttribute('data-user-email');
                    document.getElementById('viewBirthday').textContent = this.getAttribute('data-birthday');
                });
            });

            // Add event listeners for edit buttons
            document.querySelectorAll('[data-bs-target="#editUserModal"]').forEach(button => {
                button.addEventListener('click', function() {
                    document.getElementById('editUserId').value = this.getAttribute('data-user-id');
                    document.getElementById('editUserName').value = this.getAttribute('data-user-name');
                    document.getElementById('editUserTier').value = this.getAttribute('data-user-tier');
                    document.getElementById('editStartWork').value = this.getAttribute('data-start-work');
                    document.getElementById('editUserRole').value = this.getAttribute('data-user-role');
                    document.getElementById('editUserEmail').value = this.getAttribute('data-user-email');
                    document.getElementById('editBirthday').value = this.getAttribute('data-birthday');
                });
            });

            // Update user button event
            document.getElementById('updateUserBtn').addEventListener('click', function() {
                const userId = document.getElementById('editUserId').value;
                const userName = document.getElementById('editUserName').value;
                const userTier = document.getElementById('editUserTier').value;
                const startWork = document.getElementById('editStartWork').value;
                const userRole = document.getElementById('editUserRole').value;
                const userEmail = document.getElementById('editUserEmail').value;
                const birthday = document.getElementById('editBirthday').value;
                
                // Here you would typically send the data to the server
                console.log('Updating user:', { userId, userName, userTier, startWork, userRole, userEmail, birthday });
                
                // Close the modal
                document.getElementById('editUserModal').querySelector('.btn-close').click();
                
                // Show success message (in a real app)
                alert('User updated successfully!');
            });
        </script>
